{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Select All",
      "type": "process",
      "command": "${command:editor.action.selectAll}",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "label": "AI Chat",
      "type": "process",
      "command": "${command:aichat.addsymbolstonewchataction}",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "label": "AI Chat Focus",
      "type": "process",
      "command": "${command:aichat.focuschatpaneaction}",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "label": "AI Chat Paste",
      "type": "process",
      "command": "${command:editor.action.clipboardPasteAction}",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "label": "AI Chat Submit",
      "type": "shell",
      "command": "osascript -e 'tell application \"System Events\" to keystroke return using {command down}'",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    }
  ]
}
