 +  const handleSort = useCallback((key) => {
 +    let direction = 'asc';
 +    if (sortConfig.key === key) {
 +      if (sortConfig.direction === 'asc') {
 +        direction = 'desc';
 +      } else if (sortConfig.direction === 'desc') {
 +        direction = '';
 +      }
 +    } else {
